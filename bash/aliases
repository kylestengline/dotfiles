# -*- sh -*-

#Random
alias f='touch'
alias d='mkdir'
alias get_mail='open -a Mail'
alias get_notes='open -a Notes'
alias get_slack='open -a Slack'

#Moving around
alias bb='cd ../../'
alias bbb='cd ../../../'
alias bbbb='cd ../../../../'
alias root='cd ~/'

#Vim
alias v='vim'

#Rails
alias r='rails'

#Bundler
alias bi='bundle install'
alias bx='bundle exec'
alias bu='bundle update'

#Brew
alias bri='brew install'

#React
alias ns='npm start'
alias nu='npm update'
alias ni='npm install'

#Projects
alias motivatedir='cd ~/projects/motivate/'
alias simondir='cd ~/projects/kylestengline.github.io'
alias blogdir='cd ~/projects/blog_app_tests'
alias kitdir='cd ~/projects/keep_in_touch'
alias portdir='cd ~/projects/portfolio/'
alias myscripts='cd /usr/local/bin/scripts'
alias myscriptgit='cd ~/projects/scripts/'
alias exerdir='cd ~/projects/exercism/ruby/ && ls'
alias get_github='open https://github.com/kylestengline'
alias proj='cd ~/projects/ && ls'

#Git
alias g='git'
alias gcb='g checkout -b'
alias gc='g checkout'
alias ga='g add .'
alias gcm='g commit -m'
alias gs='g status'
alias gb='g branch'
alias grv='g remote -v'
alias gm='g merge'
alias gp='g push'
alias gpo='g push origin'
alias gpu='g push upstream'
alias gclo='g clone'
